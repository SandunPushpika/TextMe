{
  "manifest_version": 3,
  "name": "textMe",
  "description": "Screenshot to Text",
  "version": "1.0",
  "action": {
    "default_title": "click to get screenshot",
    "default_icon": "Icons/text.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "scripts/background.js"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; img-src 'self' data:;"
  }
}